@using Ad_Tools.Models
@model LogListModel
@{
    ViewBag.Title = "Activity And Log";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div>

    <!-- CONTROL HEIGHT SIZING -->
    <div class="widget">
        <div class="title">
            <h3>Activity And Log</h3>
        </div>
        <div class="titlebar"> Last Action Completed </div>
        <center>
            <input id="ClearBtn" type="submit" class="btn" value="ClearLog" onclick="Log_Clear()">
            @Html.DropDownList("Logfile", Model.filelist, htmlAttributes: new {})
        </center>
        <div id="tablespace" style="margin-top:25px;">
            <table id="example" class="display" cellspacing="0" width="100%">

                <thead>
                    <tr>
                        <th>Owner</th>
                        <th>Date</th>
                        <th>Target</th>
                        <th>Time</th>
                        <th>Request Type</th>
                        <th>Gpt Object</th>
                        
                        <th>Status</th>
                        <th>Status Code</th>
                        <th>Description</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.loglist)
                    {
                        <tr>
                            <td>@item.Operator</td>
                            <td>@item.Date</td>
                            <td></td>
                            <td></td>
                            <td></td>
                            <td>@item.Time</td>
                            <td>@item.Status</td>
                            <td></td>
                            <td>@item.Description</td>
                        </tr>

                    }
                </tbody>

            </table>
        </div>
    </div>
</div>

@Scripts.Render("~/Scripts/adtools/jquery-2.1.0.min.js")
@Scripts.Render("~/Scripts/adtools/adtools.js")
@Scripts.Render("~/Scripts/adtools/bootstrap.js")

