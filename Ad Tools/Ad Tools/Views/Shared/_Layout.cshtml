                                    </ul>
                                </li>
                                
                            </ul>
                        </nav>
                      
                    </div>
                   
                    <div class="col-md-10 content-wrapper">
                        <div class="row line">
                            <div class="col-lg-4 ">
                                <ul class="breadcrumb">
                                    <li class="first"><span></span><a href="~/#">Dashboard</a></li><li class="active"><span></span>ActivityandLog</li>
                                </ul>
                                @Html.Partial("_LoginPartial")
                               
                            </div>
                        </div>
                        <div class="message row">
                            <p style="margin-right:15px;float:right;margin-bottom:0;margin-top:0;visibility:hidden" >All Requests:<span id="one">0</span>&nbsp;&nbsp;&nbsp;Pending Requests:<span id="two">0</span>&nbsp;&nbsp;&nbsp;Success Requests:<span id="three">0</span>&nbsp;&nbsp;&nbsp;Failed Request:<span id="four">0</span></p>
                        </div>
                    </div>
                    <div class="col-md-10 main-content">
                        @RenderBody()
                        
                    </div>
                   
                </div>
              
            </div>
           
        </div>
     
    </div>
