    @Styles.Render("~/Scripts/adtools/main.css")
    @Styles.Render("~/Scripts/adtools/navigator.css")
    @Styles.Render("~/Scripts/adtools/content.css")
    @Styles.Render("~/Scripts/adtools/jquery.dataTables.min.css")
