    @Scripts.Render("~/Scripts/adtools/jquery-2.1.0.min.js")
    @Scripts.Render("~/Scripts/adtools/bootstrap.js")
    
    @Scripts.Render("~/Scripts/adtools/easyui/jquery.easyui.min.js")
    @Scripts.Render("~/Scripts/adtools/jquery.dataTables.min.js")
    @Scripts.Render("~/Scripts/adtools/adtools.js")
    @Scripts.Render("~/Scripts/adtools/Dashboard.js")
    @Scripts.Render("~/Scripts/adtools/User.js")
    @Scripts.Render("~/Scripts/adtools/Computer.js")
    @Scripts.Render("~/Scripts/adtools/Group.js")
