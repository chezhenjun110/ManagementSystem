@using Ad_Tools.Models

@model UserDetailModel
@{
    ViewBag.Title = "Report";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>


    <div class="widget">
        <div class="title">
            <h3> System User Number </h3>
        </div>
        <div id="messshow" style="margin:20px 0"></div>
        <div id="tablespace" style="margin-top:25px;">
            <table id="example" class="display" cellspacing="0" width="100%">

                <thead>
                    <tr>
                        <th>id</th>
                        <th>username</th>
                        <th>CreateTime</th>
                        <th>CreateBy</th>
                        <th>LatestLogin</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.userdetail)
                    {
                        <tr>
                            <td>@item.id</td>
                            <td>@item.name</td>
                            <td>@item.CreateTime</td>
                            
                            <td>@item.Createby</td>
                            <td>@item.LatestLogin</td>
                           
                           </tr> 

                    }
                </tbody>

            </table>
        </div>


    </div>

</div>



